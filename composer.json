{
  "name": "optimistdigital/nova-menu-builder",
  "description": "Menu Builder Tool for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "menu",
    "builder",
    "optimistdigital"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "optimistdigital/nova-locale-field": "^2.0.0",
    "laravel/nova": "^2.0.11 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\MenuBuilder\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\MenuBuilder\\MenuBuilderServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
