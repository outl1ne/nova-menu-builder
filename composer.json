{
  "name": "optimistdigital/nova-menu-builder",
  "description": "This Laravel Nova package allows you to create and manage menus and menu items.",
  "keywords": [
    "laravel",
    "nova",
    "menu",
    "builder",
    "optimistdigital"
  ],
  "repositories": [
    {
      "type": "path",
      "url": "../nova"
    }
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "doctrine/dbal": ">=2.0.0",
    "laravel/nova": "^4.0",
    "optimistdigital/nova-translations-loader": "^4.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\MenuBuilder\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OptimistDigital\\MenuBuilder\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\MenuBuilder\\MenuBuilderServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
